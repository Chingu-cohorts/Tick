<%= form_for @ticket do |f| %>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :is_processed %>
    <%= f.check_box :is_processed %>
  </div>

  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
